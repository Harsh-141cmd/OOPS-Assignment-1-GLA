//Question 10. Print all the Odd Numbers from 1 to N (Top to Down Approach).

import java.lang.*;
import java.util.*;

public class Main {
    public static void main(String[] args) {
        // YOUR CODE GOES HERE
        
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        int i = 1;

        while(i<=N){
            System.out.print(i+" ");
            i = i+2;
        }
    }
}
