//Question 7. Print the Multiplication table of N number.

import java.lang.*;
import java.util.*;

public class Main {
    public static void main(String[] args) {
        // YOUR CODE GOES HERE
        
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();

        for(int i=1;i<=10;i++){
            System.out.println(N+" "+"*"+" "+i+" "+"="+" "+N*i);
        }
    }
}
