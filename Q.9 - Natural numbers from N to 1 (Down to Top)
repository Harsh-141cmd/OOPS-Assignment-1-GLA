//Question 9. Print all the Natural numbers from N to 1 (Down to Top Approach).

import java.lang.*;
import java.util.*;

public class Main {
    public static void main(String[] args) {
        // YOUR CODE GOES HERE
        
        Scanner sc = new Scanner(System.in);

        int N=sc.nextInt();
        int i=1;
        
        while(i<=N){
            System.out.print(N+" ");
            N--;
        }
    }
}
